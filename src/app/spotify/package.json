{
  "name": "qr-code-pose-estimation",
  "version": "1.0.0",
  "description": "QR Code Pose Estimation using JSQR and JIMP",
  "main": "index.js",
  "scripts": {
    "json-server": "json-server --port 5000  markerLinks.json",
    "manage": "parcel ./manage.html --open",
    "start-db": "json-server --port 5000 markerLinks.json",
    "start-pdf": "ruby pdfgen.rb",
    "start-editor": "yarn manage",
    "start-app": "yarn exec parcel index.html",
    "build-cards": "concurrently \"yarn start-db\" \"yarn start-pdf\" \"yarn start-editor\"",
    "run-app": "yarn start-db && yarn start-app"
  },
  "dependencies": {
    "jimp": "^0.22.12",
    "jsqr": "^1.4.0",
    "opencv.js": "^4.5.5",
    "json-server": "^0.17.1"
  },
  "devDependencies": {
    "parcel": "^2.8.3",
    "concurrently": "^6.2.1",
    "@babel/core": "^7.20.0",
    "@babel/preset-react": "^7.20.2"
  }
  
}
