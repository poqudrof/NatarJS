{
  "name": "camera-projector-calibration-v1",
  "version": "1.0.0",
  "description": "Complete camera/projector calibration system with Firebase integration",
  "main": "src/components/CalibrationLib.js",
  "type": "module",
  "scripts": {
    "start": "parcel public/index.html --open",
    "build": "parcel build public/index.html",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "dev": "parcel watch public/index.html",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "camera",
    "projector",
    "calibration",
    "opencv",
    "firebase",
    "computer-vision",
    "augmented-reality"
  ],
  "author": "QR Pose Estimation Team",
  "license": "MIT",
  "dependencies": {
    "firebase": "^10.12.2",
    "jsqr": "^1.4.0",
    "jimp": "^0.22.0",
    "jsdom": "^22.0.0"
  },
  "devDependencies": {
    "parcel": "^2.8.3",
    "jest": "^29.0.0",
    "playwright": "^1.40.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}